{
  "name": "segedin",
  "description": "Type-safe getters/setters for nested object structures",
  "version": "1.2.0",
  "author": "Adam StanÄ›k <adam.stanek@v3net.cz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/adam-stanek/segedin.git"
  },
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "ava": "1.0.0-beta.8",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "ttypescript": "^1.5.5",
    "typescript": "^3.1.2"
  },
  "scripts": {
    "build:lib": "tsc -p lib",
    "build:transform": "tsc -p ts-transform",
    "test": "ava",
    "test:transform": "TS_NODE_COMPILER=ttypescript ava",
    "prettier": "prettier --write \"lib/**/*.ts\" \"ts-transform/**/*.ts\" \"test/**/*.{js,ts}\" README.md tsconfig.json .prettierrc"
  },
  "ava": {
    "require": [
      "./test/bootstrap"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "test/*.test.ts"
    ],
    "sources": [
      "lib/*.js"
    ],
    "compileEnhancements": false
  }
}
