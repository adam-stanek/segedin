{
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.3",
    "ava": "1.0.0-beta.8",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "ttypescript": "^1.5.5",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "test": "ava",
    "test:transform": "TS_NODE_COMPILER=ttypescript ava",
    "prettier": "prettier --write \"lib/**/*.ts\" \"test/**/*.{js,ts}\" README.md tsconfig.json .prettierrc"
  },
  "ava": {
    "require": [
      "./test/bootstrap"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "test/*.test.ts"
    ],
    "sources": [
      "lib/*.js"
    ],
    "compileEnhancements": false
  }
}
